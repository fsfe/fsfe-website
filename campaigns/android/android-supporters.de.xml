    <image>/campaigns/android/foebud.png</image>
    <link>https://digitalcourage.de</link>
    <descr>Digitale Courage e.V.</descr>
  </supporter>
  
  <supporter id="ansol" order="2">
    <name>ANSOL</name>
    <image>/campaigns/android/ansol.png</image>
