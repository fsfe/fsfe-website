    <descr>Beschreibung</descr>
  </supporter>
  
  <supporter id="fsf" order="3">
    <name>FSF</name>
    <image>/campaigns/android/fsf.png</image>
    <link>https://fsf.org</link>
    <descr>Freie Weichwaren Stiftung</descr>
  </supporter>
  
</supporterset>
